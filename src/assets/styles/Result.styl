.Results li
    list-style: none

.User__container
    background: white
    width: calc(100%-2px)
    height 65px
    border: 1px solid black
    display: grid
    justify-content: center
    align-items: center
    grid-template-columns: 40% 1fr

.User
    background: white
    display: flex
    align-content: center

.User img 
    width: 50px 
    margin-left: 20px
    margin-right: 20px

.User h3
    align-self: center

.PageList
    justify-content: center
    width: 100%
    height 30px
    margin: auto
    text-align: center
    cursor: pointer
    display: flex
    align-items: center

.PageList ul
    list-style: none
    display: flex
    height 100%
    background: #dcdada
    display: flex
    justify-content: center
    align-items: center


.PageList ul li
    width: 40px
    height 100%
    display: flex
    justify-content: center
    align-items: center

.active
    background: green

.Repos
    overflow-x: auto 
    display: grid
    grid-template-columns:50% 50%

